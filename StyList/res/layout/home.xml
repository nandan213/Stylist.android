<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blackbg"
    tools:context=".LoginActivity" >

    <include
        android:id="@+id/header_lyt"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/title_bar" >
    </include>

    <RelativeLayout
        android:id="@+id/data_list"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/header_lyt"
        android:layout_margin="10dip" >

        <RelativeLayout
            android:id="@+id/layoutone"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@color/text_white" >

            <ImageView
                android:id="@+id/user_pic"
                android:layout_width="95dp"
                android:layout_height="81dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:paddingBottom="5dp"
                android:scaleType="fitXY"
                android:src="@drawable/userpic" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@+id/user_pic" >

                <TextView
                    android:id="@+id/user_add"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/username"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:ems="8"
                    android:text="" />

                <TextView
                    android:id="@+id/user_phno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/user_add"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:ems="8"
                    android:text="" />

                <TextView
                    android:id="@+id/username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:ems="7"
                    android:text=""
                    android:textColor="@color/black_text"
                    android:textSize="15dp" />
            </RelativeLayout>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/leftdraw" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layouttwo"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layoutone"
            android:layout_marginTop="10dp"
            android:background="@color/text_white" >

            <TextView
                android:id="@+id/Mission_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:ems="8"
                android:text="Mission"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_Mission"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Mission_txt"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:ems="8"
                android:paddingBottom="5dp"
                android:text=""
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/leftdraw" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layoutthree"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layouttwo"
            android:layout_marginTop="10dp"
            android:background="@color/text_white" >

            <TextView
                android:id="@+id/shop_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:ems="8"
                android:text="Shop"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_shopname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/shop_txt"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:ems="8"
                android:paddingBottom="5dp"
                android:text=""
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/user_shopaddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/user_shopname"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:ems="12"
                android:paddingBottom="5dp"
                android:text=""
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/leftdraw" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layoutfour"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/layoutthree"
            android:layout_marginTop="10dp"
            android:background="@color/text_white" >

            <TextView
                android:id="@+id/todaysList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="5dp"
                android:ems="8"
                android:text="Shop"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/todayCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/todaysList"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="15dp"
                android:ems="8"
                android:paddingBottom="5dp"
                android:text="1"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ClientFollowing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="5dp"
                android:ems="12"
                android:paddingBottom="5dp"
                android:paddingLeft="60dip"
                android:text="Client Following"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ClientFollowingCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/ClientFollowing"
                android:layout_marginTop="5dp"
                android:ems="12"
                android:paddingLeft="100dip"
                android:paddingTop="6dip"
                android:text="4"
                android:textColor="@color/black_text"
                android:textSize="14sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/linearForSchedule"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/layoutfour"
            android:orientation="horizontal" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:text="8am-5pm"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tue" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:text="8am-5pm"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Wed" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:text="8am-5pm"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Thu" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:text="8am-5pm"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fri" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:text="8am-5pm"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Sat" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:layout_marginLeft="10dip"
                    android:text="Off"
                    android:textSize="15sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dip" >

                <Button
                    android:id="@+id/8am-5pm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Sun" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/8am-5pm"
                    android:layout_marginLeft="15dip"
                    android:text="Off"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/relativeCashFlow"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/linearForSchedule" >

            <TextView
                android:id="@+id/cashFlowText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="CashFlow" />

            <ImageView
                android:id="@+id/CashFlowSeprator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/cashFlowText"
                android:background="@drawable/sepraterwhite" />

            <LinearLayout
                android:id="@+id/cahsFlowlinearID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dip"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textIDcashFlowDaily"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Daily" />

                <TextView
                    android:id="@+id/textIDcashFlowDailyAmount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="300" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textIDcashFlowDaily"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Deduction" />

                <TextView
                    android:id="@+id/textIDcashFlowDailyAmount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="300" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="30dip"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textIDcashFlowTotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total" />

                <TextView
                    android:id="@+id/textIDcashFlowTotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="300" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/Services"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@id/relativeCashFlow"
            android:background="@drawable/servicewhite" >

            <LinearLayout
                android:id="@+id/servicesLinearID1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dip"
                    android:layout_marginTop="30dip"
                    android:text="Children" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dip"
                    android:text="$15" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/servicesLinearID2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="80dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dip"
                    android:text="Fade" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="$15" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/servicesLinearID2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="30dip"
                    android:layout_marginTop="30dip"
                    android:text="Razor" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="$15" 
                    android:layout_marginTop="30dip"
                    />
            </LinearLayout>

           
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>